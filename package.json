{
  "name": "@springdragon/chat-ui",
  "version": "0.0.13",
  "private": false,
  "type": "module",
  "main": "lib/chatui.umd.js",
  "module": "lib/chatui.js",
  "style": "lib/style.css",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/chatui.js",
      "require": "./lib/chatui.umd.js",
      "types": "./lib/index.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "vue": "^3.4.15"
  },
  "description": "VueChatUI - 一个现代的 Vue 3 组件库，专为构建交互式聊天界面设计。结合 Vue 3 的响应式特性和高性能，提供了一套丰富、灵活且易于集成的聊天 UI 组件，适用于构建高度交互和定制化的聊天应用。",
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "global.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aa386400024/chatui-vue3.git"
  },
  "keywords": [
    "vue",
    "ui-components",
    "vue3",
    "typescript"
  ],
  "author": "zcl",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "storybook-i18n": "^2.1.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@storybook/vue3": "^7.6.17",
    "@storybook/vue3-vite": "^7.6.17",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.26",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.71.1",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.17",
    "typescript": "~5.3.3",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}